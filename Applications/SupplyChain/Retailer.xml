<?xml version="1.0" encoding="UTF-8"?>
<tns:Service xmlns:tns="http://www.example.org/ServiceSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.example.org/ServiceSchema ../../src/scripts/Schema/ServiceSchema.xsd ">
  <tns:Name>Retailer</tns:Name>
  <tns:Variables>
    <tns:Variable>
      <tns:Name>amount</tns:Name>
      <tns:Type>INTEGER</tns:Type>
      <tns:Value>-1</tns:Value>
    </tns:Variable>
    <tns:Variable>
      <tns:Name>product_type</tns:Name>
      <tns:Type>INTEGER</tns:Type>
      <tns:Value>-1</tns:Value>
    </tns:Variable>
    <tns:Variable>
      <tns:Name>stockA</tns:Name>
      <tns:Type>INTEGER</tns:Type>
      <tns:Value>-1</tns:Value>
    </tns:Variable>
    <tns:Variable>
      <tns:Name>stockB</tns:Name>
      <tns:Type>INTEGER</tns:Type>
      <tns:Value>-1</tns:Value>
    </tns:Variable>
    <tns:Variable>
      <tns:Name>stockC</tns:Name>
      <tns:Type>INTEGER</tns:Type>
      <tns:Value>-1</tns:Value>
    </tns:Variable>
    <tns:Variable>
      <tns:Name>requested</tns:Name>
      <tns:Type>INTEGER</tns:Type>
      <tns:Value>-1</tns:Value>
    </tns:Variable>
    <tns:Variable>
      <tns:Name>deliver</tns:Name>
      <tns:Type>INTEGER</tns:Type>
      <tns:Value>-1</tns:Value>
    </tns:Variable>
    <tns:Variable>
      <tns:Name>complemented</tns:Name>
      <tns:Type>INTEGER</tns:Type>
      <tns:Value>-1</tns:Value>
    </tns:Variable>
    <tns:Variable>
      <tns:Name>manufacturer_stock</tns:Name>
      <tns:Type>INTEGER</tns:Type>
      <tns:Value>-1</tns:Value>
    </tns:Variable>
    <tns:Variable>
      <tns:Name>factoryA_can_produce</tns:Name>
      <tns:Type>INTEGER</tns:Type>
      <tns:Value>-1</tns:Value>
    </tns:Variable>
    <tns:Variable>
      <tns:Name>factoryB_can_produce</tns:Name>
      <tns:Type>INTEGER</tns:Type>
      <tns:Value>-1</tns:Value>
    </tns:Variable>
    <tns:Variable>
      <tns:Name>factoryC_can_produce</tns:Name>
      <tns:Type>INTEGER</tns:Type>
      <tns:Value>-1</tns:Value>
    </tns:Variable>
    <tns:Variable>
      <tns:Name>ICMutationVariable</tns:Name>
      <tns:Type>INTEGER</tns:Type>
      <tns:Value>0</tns:Value>
    </tns:Variable>
    <tns:Variable>
      <tns:Name>RandomVariable</tns:Name>
      <tns:Type>INTEGER</tns:Type>
      <tns:Value>1~99</tns:Value>
    </tns:Variable>
  </tns:Variables>
  <tns:Activities>
    <tns:Activity>
      <tns:Name>task1</tns:Name>
      <tns:Type>ReceiveTask</tns:Type>
      <tns:Effect>receive product_type amount manufacturer_stock factoryA_can_produce factoryB_can_produce factoryC_can_produce</tns:Effect>
    </tns:Activity>
    <tns:Activity>
      <tns:Name>task2</tns:Name>
      <tns:Type>ReceiveTask</tns:Type>
      <tns:Effect>receive stockA stockB stockC</tns:Effect>
    </tns:Activity>
    <tns:Activity>
      <tns:Name>task3</tns:Name>
      <tns:Type>LocalTask</tns:Type>
      <tns:Effect><![CDATA[deliver := amount ; stockA := stockA - amount ;]]></tns:Effect>
    </tns:Activity>
    <tns:Activity>
      <tns:Name>task4</tns:Name>
      <tns:Type>SendTask</tns:Type>
      <tns:Effect>send deliver</tns:Effect>
    </tns:Activity>
    <tns:Activity>
      <tns:Name>task5</tns:Name>
      <tns:Type>LocalTask</tns:Type>
      <tns:Effect><![CDATA[deliver := stockA ; requested := amount - stockA ; stockA := 0;]]></tns:Effect>
    </tns:Activity>
    <tns:Activity>
      <tns:Name>task6</tns:Name>
      <tns:Type>SendTask</tns:Type>
      <tns:Effect>send requested</tns:Effect>
    </tns:Activity>
    <tns:Activity>
      <tns:Name>task7</tns:Name>
      <tns:Type>ReceiveTask</tns:Type>
      <tns:Effect>receive complemented</tns:Effect>
    </tns:Activity>
    <tns:Activity>
      <tns:Name>task8</tns:Name>
      <tns:Type>LocalTask</tns:Type>
      <tns:Effect><![CDATA[deliver := deliver + complemented ;]]></tns:Effect>
    </tns:Activity>
    <tns:Activity>
      <tns:Name>task9</tns:Name>
      <tns:Type>ReceiveTask</tns:Type>
      <tns:Effect>receive stockB stockA stockC</tns:Effect>
    </tns:Activity>
    <tns:Activity>
      <tns:Name>task10</tns:Name>
      <tns:Type>LocalTask</tns:Type>
      <tns:Effect><![CDATA[deliver := amount ; stockB := stockB - amount ;]]></tns:Effect>
    </tns:Activity>
    <tns:Activity>
      <tns:Name>task11</tns:Name>
      <tns:Type>LocalTask</tns:Type>
      <tns:Effect><![CDATA[deliver := stockB ; requested := amount - stockB ; stockB := 0;]]></tns:Effect>
    </tns:Activity>
    <tns:Activity>
      <tns:Name>task12</tns:Name>
      <tns:Type>ReceiveTask</tns:Type>
      <tns:Effect>receive stockC stockA stockB</tns:Effect>
    </tns:Activity>
    <tns:Activity>
      <tns:Name>task13</tns:Name>
      <tns:Type>LocalTask</tns:Type>
      <tns:Effect><![CDATA[deliver := amount ; stockC := stockC - amount ;]]></tns:Effect>
    </tns:Activity>
    <tns:Activity>
      <tns:Name>task14</tns:Name>
      <tns:Type>LocalTask</tns:Type>
      <tns:Effect><![CDATA[deliver := stockC ; requested := amount - stockC ; stockC := 0 ;]]></tns:Effect>
    </tns:Activity>
  </tns:Activities>
  <tns:Transitions>
    <tns:Transition>
      <tns:Name>t1</tns:Name>
      <tns:Source>task1</tns:Source>
      <tns:Sink>task2</tns:Sink>
      <tns:Guard><![CDATA[product_type == 0]]></tns:Guard>
    </tns:Transition>
    <tns:Transition>
      <tns:Name>t2</tns:Name>
      <tns:Source>task2</tns:Source>
      <tns:Sink>task3</tns:Sink>
      <tns:Guard><![CDATA[stockA >= amount]]></tns:Guard>
    </tns:Transition>
    <tns:Transition>
      <tns:Name>t3</tns:Name>
      <tns:Source>task3</tns:Source>
      <tns:Sink>task4</tns:Sink>
      <tns:Guard>true</tns:Guard>
    </tns:Transition>
    <tns:Transition>
      <tns:Name>t4</tns:Name>
      <tns:Source>task2</tns:Source>
      <tns:Sink>task5</tns:Sink>
      <tns:Guard><![CDATA[stockA < amount]]></tns:Guard>
    </tns:Transition>
    <tns:Transition>
      <tns:Name>t5</tns:Name>
      <tns:Source>task5</tns:Source>
      <tns:Sink>task6</tns:Sink>
      <tns:Guard>true</tns:Guard>
    </tns:Transition>
    <tns:Transition>
      <tns:Name>t6</tns:Name>
      <tns:Source>task6</tns:Source>
      <tns:Sink>task7</tns:Sink>
      <tns:Guard>true</tns:Guard>
    </tns:Transition>
    <tns:Transition>
      <tns:Name>t7</tns:Name>
      <tns:Source>task7</tns:Source>
      <tns:Sink>task8</tns:Sink>
      <tns:Guard>true</tns:Guard>
    </tns:Transition>
    <tns:Transition>
      <tns:Name>t8</tns:Name>
      <tns:Source>task8</tns:Source>
      <tns:Sink>task4</tns:Sink>
      <tns:Guard>true</tns:Guard>
    </tns:Transition>
    <tns:Transition>
      <tns:Name>t9</tns:Name>
      <tns:Source>task1</tns:Source>
      <tns:Sink>task9</tns:Sink>
      <tns:Guard><![CDATA[product_type == 1]]></tns:Guard>
    </tns:Transition>
    <tns:Transition>
      <tns:Name>t10</tns:Name>
      <tns:Source>task9</tns:Source>
      <tns:Sink>task10</tns:Sink>
      <tns:Guard><![CDATA[stockB >= amount]]></tns:Guard>
    </tns:Transition>
    <tns:Transition>
      <tns:Name>t11</tns:Name>
      <tns:Source>task10</tns:Source>
      <tns:Sink>task4</tns:Sink>
      <tns:Guard>true</tns:Guard>
    </tns:Transition>
    <tns:Transition>
      <tns:Name>t12</tns:Name>
      <tns:Source>task9</tns:Source>
      <tns:Sink>task11</tns:Sink>
      <tns:Guard><![CDATA[stockB < amount]]></tns:Guard>
    </tns:Transition>
    <tns:Transition>
      <tns:Name>t13</tns:Name>
      <tns:Source>task11</tns:Source>
      <tns:Sink>task6</tns:Sink>
      <tns:Guard>true</tns:Guard>
    </tns:Transition>
    <tns:Transition>
      <tns:Name>t14</tns:Name>
      <tns:Source>task1</tns:Source>
      <tns:Sink>task12</tns:Sink>
      <tns:Guard><![CDATA[product_type == 2]]></tns:Guard>
    </tns:Transition>
    <tns:Transition>
      <tns:Name>t15</tns:Name>
      <tns:Source>task12</tns:Source>
      <tns:Sink>task13</tns:Sink>
      <tns:Guard><![CDATA[stockC >= amount]]></tns:Guard>
    </tns:Transition>
    <tns:Transition>
      <tns:Name>t16</tns:Name>
      <tns:Source>task13</tns:Source>
      <tns:Sink>task4</tns:Sink>
      <tns:Guard>true</tns:Guard>
    </tns:Transition>
    <tns:Transition>
      <tns:Name>t17</tns:Name>
      <tns:Source>task12</tns:Source>
      <tns:Sink>task14</tns:Sink>
      <tns:Guard><![CDATA[stockC < amount]]></tns:Guard>
    </tns:Transition>
    <tns:Transition>
      <tns:Name>t18</tns:Name>
      <tns:Source>task14</tns:Source>
      <tns:Sink>task6</tns:Sink>
      <tns:Guard>true</tns:Guard>
    </tns:Transition>
  </tns:Transitions>
  <tns:StartTask>task1</tns:StartTask>
</tns:Service>
